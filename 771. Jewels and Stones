class Solution {
public:
    int numJewelsInStones(string J, string S) {
    const int c = 128;
    const int n = 50;
    int Jw[c];

    for (int i=0; i<c; i++)
    {
        Jw[i]=0;
    }
    int s = 0;
    int ch = 0;
    int i=0;
    while ((i<n) and (J[i] != 0))
    {
        ch = J[i];
        if (Jw[ch] == 0)
            Jw[ch] = 1;
        i++;
    };
    i=0;
    while ((i<c) and (S[i]!=0))
    {
        ch = S[i];
        if (Jw[ch]==1)
            s++;
        i++;
    }
    return s;
        
    }
};
