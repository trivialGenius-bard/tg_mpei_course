// https://leetcode.com/problems/remove-duplicates-from-sorted-array/
class Solution {
public:
int removeDuplicates(vector<int>& nums) {
for(int i = nums.size() - 1; i > 0; i--){
if(nums[i] == nums[i-1]){
for(int j = i; j < nums.size() - 1; j++){
nums[j] = nums[j+1];
}
nums.pop_back();
}
}
return nums.size();
}
};
